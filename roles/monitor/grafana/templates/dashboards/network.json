        {
          "expr": "rate(ifInOctets[2m])",
          "format": "time_series",
          "group": [],
          "intervalFactor": 1,
          "legendFormat": "{{snmp_target}} {{ifIndex}} rx",
          "metricColumn": "CONCAT(labels->>'snmp_target', ' - ', labels->>'ifIndex')",
          "rawQuery": true,
          "rawSql": "SELECT\n  time,\n  metric,\n  ((\n    CASE\n      WHEN value >= lag(value) OVER w\n        THEN value - lag(value) OVER w\n      WHEN lag(value) OVER w IS NULL THEN NULL\n      ELSE value\n    END\n  ) * 8) / EXTRACT(EPOCH FROM time - lag(time) OVER w) AS \"value\"\nFROM (\n  SELECT\n    CONCAT(labels->>'snmp_target', ' - ', labels->>'ifIndex') AS \"metric\",\n    $__timeGroupAlias(\"time\", $__interval, previous),\n    SUM(value) AS \"value\"\n  FROM prometheus.metrics\n  WHERE\n    $__timeFilter(\"time\")\n    AND name='ifInOctets'\n    AND value!='NaN'\n  GROUP BY metric, time\n  ORDER BY metric, time\n) AS samples\nWINDOW w AS (PARTITION BY metric ORDER BY time)\nORDER BY metric, time;",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "metrics",
          "timeColumn": "\"time\"",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            },
            {
              "datatype": "text",
              "name": "",
              "params": [
                "name",
                "=",
                "'ifInOctets'"
              ],
              "type": "expression"
            }
          ]
        }
